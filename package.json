{
  "name": "seawater-climate-risk-platform",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:ui": "playwright test --ui",
    "build:ci": "node concat_ci.js",
    "test:callisto": "playwright test tests/callisto-integration-platform.spec.ts",
    "test:mapping": "playwright test tests/callisto-integration-mapping.spec.ts",
    "test:lifecycle": "playwright test tests/callisto-integration-lifecycle.spec.ts",
    "test:certificates": "playwright test tests/callisto-certificate-management.spec.ts",
    "test:overrides-email": "playwright test tests/callisto-client-overrides-email.spec.ts",
    "test:core-functions": "playwright test tests/callisto-core-happyhippo-functions.spec.ts",
    "test:integration-suite": "playwright test tests/callisto-*.spec.ts",
    "test:live-system": "playwright test tests/live-system-integration.spec.ts",
    "test:database": "playwright test tests/database-integration.spec.ts",
    "test:all-callisto": "playwright test tests/callisto-*.spec.ts tests/live-system-integration.spec.ts tests/database-integration.spec.ts",
    "test:live-only": "playwright test tests/live-system-integration.spec.ts tests/database-integration.spec.ts",
    "test:debug": "playwright test --debug",
    "test:load-data": "node db/test_data/run_load_callisto_integration_examples.js",
    "test:setup": "mkdir -p test-results/screenshots",
    "test:integrations": "node src/backend/src/integrations/test-basic.js",
    "climate:examples": "node src/backend/src/integrations/examples/basic-usage.js",
    "climate:health": "node -e \"const {ClimateDataIntegration} = require('./src/backend/src/integrations'); const c = new ClimateDataIntegration(); c.getHealthStatus().then(h => console.log(JSON.stringify(h, null, 2))).finally(() => c.destroy());\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "@tanstack/react-query": "^4.28.0",
    "@tanstack/react-query-devtools": "^4.28.0",
    "mapbox-gl": "^2.15.0",
    "react-map-gl": "^7.0.21",
    "chart.js": "^4.2.1",
    "react-chartjs-2": "^5.2.0",
    "framer-motion": "^10.12.16",
    "lucide-react": "^0.244.0",
    "clsx": "^1.2.1",
    "class-variance-authority": "^0.6.0",
    "react-hook-form": "^7.43.9",
    "@hookform/resolvers": "^3.1.0",
    "zod": "^3.21.4",
    "date-fns": "^2.30.0",
    "react-hot-toast": "^2.4.1",
    "react-helmet-async": "^1.3.0",
    "react-intersection-observer": "^9.4.3",
    "react-window": "^1.8.8",
    "react-virtualized-auto-sizer": "^1.0.17",
    "@types/react-table": "^7.7.20",
    "js-yaml": "^4.1.0",
    "pg": "^8.16.0",
    "react-table": "^7.8.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/mapbox-gl": "^2.7.10",
    "@types/react-window": "^1.8.5",
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^4.3.2",
    "typescript": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "vitest": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "jsdom": "^21.1.1",
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0"
  }
}